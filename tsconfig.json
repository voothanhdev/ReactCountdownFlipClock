{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "jsx": "react",
        "declaration": true,
        "declarationDir": "./dist/types",
        "outDir": "./dist",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@app/*": ["./src/*"],
        }
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
